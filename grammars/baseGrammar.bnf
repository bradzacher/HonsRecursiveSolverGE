<individual>      ::=\
if (<condition>) {\n\
    return <base>;\n\
} else {\n\
    return <expr>;\n\
}\n



<condition>       ::= <var> <logic_op> <digit>

<logic_op>        ::= "<" | ">"

<op>              ::= "'-'" | "'*'" | "'+'" | "'/'"

<digit>           ::= 1 | 2 | <big_digit>
<big_digit>       ::= 3 | 4 | 5 | <bigger_digit>
<bigger_digit>    ::= 6 | 7 | <huge_digit>
<huge_digit>      ::= 8 | 9 | 0

<var>             ::= x

<base>            ::= <digit> | <var>



<expr>            ::= 